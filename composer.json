{
  "name": "lujo/laravel-rest",
  "description": "Laravel package for creating simple REST API",
  "type": "library",
  "version": "1.2.2",
  "keywords": [
    "Laravel",
    "REST",
    "API"
  ],
  "require": {
    "php": ">=8.0",
    "laravel/framework": ">=v10.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.4-dev"
  },
  "autoload": {
    "psr-4": {
      "Lujo\\Laravel\\Rest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lujo\\Laravel\\Rest\\Test\\": "tests"
    }
  },
  "authors": [
    {
      "name": "lujo",
      "email": "lukamatosevic5@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
        "Lujo\\Laravel\\Rest\\LaravelRestServiceProvider"
      ]
    }
  }
}
